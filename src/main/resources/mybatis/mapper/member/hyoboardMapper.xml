<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hyoMapper">
	
	
	<insert id="insertBoard">
		INSERT INTO hyo_board(title,content,reg_date,mod_date,id)VALUES(#{title},#{content},#{reg_date},#{mod_date},#{id})
	</insert>
	
	<select id="selectList" resultType="com.eduDashboardProject.hyo.vo.HyoboardVO">
		SELECT * FROM hyo_board
		ORDER BY BNO+1 DESC
	</select>
	
	
	
</mapper>